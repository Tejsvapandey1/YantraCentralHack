generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model vulnerable_kit {
  id            BigInt    @id @default(autoincrement())
  created_at    DateTime? @default(now()) @db.Timestamptz(6)
  device        String?
  heartrate     Int?
  spo2          Int?
  bodytemp      Float?
  stepcount     Int?
  env_pressure  Float?
  latitude      Float?
  longitude     Float?
  fall_detected Boolean?
}
